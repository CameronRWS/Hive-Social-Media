<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Hive.HiveActivity">

    <ImageButton
        android:id="@+id/hiveHeaderActual"
        android:layout_width="405dp"
        android:layout_height="151dp"
        android:background="@null"
        android:scaleType="fitXY"
        android:src="@null"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/editHiveHeader"
        android:layout_width="409dp"
        android:layout_height="163dp"
        android:background="@null"
        android:scaleType="fitXY"
        android:src="@drawable/editheader"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/hivePictureBorder"
        android:layout_width="146dp"
        android:layout_height="126dp"
        android:layout_marginTop="100dp"
        android:background="@null"
        android:scaleType="fitXY"
        android:src="@drawable/profpicborder"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/hiveHeaderActual" />

    <ImageButton
        android:id="@+id/hivePictureActual"
        android:layout_width="121dp"
        android:layout_height="106dp"
        android:layout_marginBottom="20dp"
        android:background="@null"
        android:scaleType="fitXY"
        android:src="@null"
        app:layout_constraintBottom_toBottomOf="@+id/hivePictureBorder"
        app:layout_constraintEnd_toEndOf="@+id/hivePictureBorder"
        app:layout_constraintStart_toStartOf="@+id/hivePictureBorder"
        app:layout_constraintTop_toTopOf="@+id/hivePictureBorder" />

    <ImageButton
        android:id="@+id/editHivePicture"
        android:layout_width="121dp"
        android:layout_height="106dp"
        android:layout_marginTop="20dp"
        android:layout_marginBottom="20dp"
        android:background="@null"
        android:scaleType="fitXY"
        android:src="@drawable/editprofpic"
        app:layout_constraintBottom_toBottomOf="@+id/hivePictureBorder"
        app:layout_constraintEnd_toEndOf="@+id/hivePictureActual"
        app:layout_constraintStart_toStartOf="@+id/hivePictureActual"
        app:layout_constraintTop_toTopOf="@+id/hivePictureActual" />

    <ImageButton
        android:id="@+id/joinHiveButton"
        android:layout_width="101dp"
        android:layout_height="42dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="30dp"
        android:layout_marginEnd="30dp"
        android:background="@null"
        android:scaleType='fitXY'
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/hivePictureBorder"
        app:layout_constraintTop_toBottomOf="@+id/hiveHeaderActual"
        app:srcCompat="@drawable/joinhivebutton" />

    <ImageButton
        android:id="@+id/joinedHiveButton"
        android:layout_width="101dp"
        android:layout_height="42dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="30dp"
        android:layout_marginEnd="30dp"
        android:background="@null"
        android:scaleType='fitXY'
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/hivePictureBorder"
        app:layout_constraintTop_toBottomOf="@+id/hiveHeaderActual"
        app:srcCompat="@drawable/joinedhivebutton" />

    <ImageButton
        android:id="@+id/requestedHiveButton"
        android:layout_width="101dp"
        android:layout_height="42dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="30dp"
        android:layout_marginEnd="30dp"
        android:background="@null"
        android:scaleType='fitXY'
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/hivePictureBorder"
        app:layout_constraintTop_toBottomOf="@+id/hiveHeaderActual"
        app:srcCompat="@drawable/requestedhive" />

    <TextView
        android:id="@+id/hiveName"
        android:layout_width="97dp"
        android:layout_height="65dp"
        android:layout_marginStart="45dp"
        android:layout_marginTop="16dp"
        android:fontFamily="@font/helveticaneuebold"
        android:lineSpacingExtra="2.5sp"
        android:text="Iowa State University"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="18"
        app:layout_constraintEnd_toStartOf="@+id/hivePictureBorder"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editHiveHeader" />

    <TextView
        android:id="@+id/hiveMemberCount"
        android:layout_width="138dp"
        android:layout_height="24dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="85dp"
        android:fontFamily="@font/helveticaneuebold"
        android:lineSpacingExtra="2.5sp"
        android:text="bees and counting"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="10sp"
        app:layout_constraintEnd_toStartOf="@+id/hivePictureBorder"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editHiveHeader" />

    <TextView
        android:id="@+id/hiveDescription"
        android:layout_width="279dp"
        android:layout_height="74dp"
        android:layout_marginStart="175dp"
        android:layout_marginTop="110dp"
        android:fontFamily="@font/helveticaneuebold"
        android:lineSpacingExtra="2.5sp"
        android:textAlignment="textStart"
        android:textColor="#000000"
        android:textSize="10sp"
        app:layout_constraintEnd_toStartOf="@+id/hivePictureBorder"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editHiveHeader" />

    <ImageView
        android:id="@+id/hivePageDivider"
        android:layout_width="279dp"
        android:layout_height="8dp"
        android:layout_marginBottom="375dp"
        android:src="@drawable/divider"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/displayLocation" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="30dp"
        android:layout_height="29dp"
        android:layout_marginStart="5dp"
        app:layout_constraintEnd_toStartOf="@+id/hiveName"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/hiveName"
        app:srcCompat="@drawable/beehive" />
</androidx.constraintlayout.widget.ConstraintLayout>